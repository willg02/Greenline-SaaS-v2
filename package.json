{
  "name": "greenline",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:api": "npm --workspace @greenline/api run dev",
    "dev:web": "npm --workspace @greenline/web run dev",
    "dev": "concurrently \"npm:dev:api\" \"npm:dev:web\"",
    "build": "npm -ws run build",
    "lint": "echo \"No lint configured yet\"",
    "format": "echo \"No formatter configured yet\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
